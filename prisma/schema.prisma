generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "mongodb"
  url       = env("DATABASE_URL")
  directUrl = env("DATABASE_URL")
}

model test_collection_1 {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  Document_Name  String @map("Document Name")
  editors_choice String
  /// Multiple data types found: String: 99.8%, Float: 0.2% out of 1000 sampled entries
  genre          Json
  platform       String
  /// Multiple data types found: Float: 0.1%, Int: 99.9% out of 1000 sampled entries
  release_day    Json
  /// Multiple data types found: Float: 0.1%, Int: 99.9% out of 1000 sampled entries
  release_month  Json
  /// Multiple data types found: Float: 0.1%, Int: 99.9% out of 1000 sampled entries
  release_year   Json
  score          Float
  score_phrase   String
  title          String
  url            String
}
